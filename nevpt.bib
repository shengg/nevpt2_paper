	file = {APS Snapshot:/home/sheng/.zotero/zotero/rt20x4vq.default/zotero/storage/TED2WZ6Q/PhysRevB.59.html:text/html;Full Text PDF:/home/sheng/.zotero/zotero/rt20x4vq.default/zotero/storage/GRCV2Z9U/Martin et al. - 1999 - Linear and nonlinear optical properties of the con.pdf:application/pdf}
}

@article{malmqvist_restricted_2002,
